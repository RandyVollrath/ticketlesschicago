# Development environment variables
# Copy to .env.local and update with real values

# Stripe API Keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Rewardful API
REWARDFUL_API_SECRET=your_rewardful_secret

# Ticketless America Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://www.ticketlessamerica.com

# MyStreetCleaning.com Integration
MSC_SUPABASE_URL=https://your-msc-project.supabase.co
MSC_SUPABASE_SERVICE_ROLE_KEY=your_msc_service_role_key

# Passkey/WebAuthn Configuration
PASSKEY_RP_ID=localhost
PASSKEY_ORIGIN=http://localhost:3000

# Google Maps API Key for geocoding
GOOGLE_API_KEY=your_google_api_key

# Notification Services
RESEND_API_KEY=your_resend_api_key
RESEND_FROM="Ticketless America <noreply@mystreetcleaning.com>"
CLICKSEND_USERNAME=your_clicksend_username
CLICKSEND_API_KEY=your_clicksend_api_key
SMS_SENDER=+18335623866

# AI Services
ANTHROPIC_API_KEY=your_anthropic_api_key

# PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=phc_your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Feature Flags
NEXT_PUBLIC_FREE_ALERTS=true
NEXT_PUBLIC_PROTECTION_WAITLIST=true
DRY_RUN=false

# Vercel Blob Storage (for permit zone documents)
BLOB_READ_WRITE_TOKEN=vercel_blob_token_here

# Admin Access
ADMIN_API_TOKEN=your_secure_admin_token_here
NEXT_PUBLIC_ADMIN_TOKEN=your_secure_admin_token_here